using System;

namespace Ejercicio8
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] numeros = { 4, 8, 6, 7 };
            for (int i = 0; i < numeros.Length; i++)
            {
                Console.WriteLine(numeros[i]);
            }
            Console.ReadKey();
        }
    }
}
